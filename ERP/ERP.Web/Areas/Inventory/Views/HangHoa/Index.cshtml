
@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Inventory/Views/Shared/Layout.cshtml";
}



<meta charset="utf-8">
<meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
<link href="~/Content/self-writing/css/HangHoaHL.css" rel="stylesheet" />
<script src="http://malsup.github.com/jquery.form.js"></script>

<div class="panel-heading">Bạn đang xem danh mục hàng hóa</div>
<br />
<div ng-app="myApp" ng-controller="hanghoaCtrl">
    <div class="content">
        <div id="quick-access">
            <form class="form-inline quick-search-form" role="form" action="#">
                <div class="form-group" style="width: 90%">
                    <input type="text" style="width: 60%" class="form-control" placeholder="Type System Code You Want To Search Here" ng-model="search_text">
                </div>
            </form>
        </div>
        <button type="button" class="btn btn-info" data-toggle="modal" data-target="#myModal">Add New Product</button>
        <br />
        <br />
        <!-- Modal Thêm mới -->
        <div class="modal fade" id="myModal" role="dialog" tabindex="-1">
            <div class="modal-dialog modal-lg" style="width: 90%">
                <div class="modal-content" style="height: 100%">

                    <div class="modal-body">
                            <fieldset style="height: 100%">
                                <!-- Form Name -->
                                <legend>Add New Product</legend>
                                <!-- Text input-->
                                <div class="col-md-6 container_left">
                                    <div class="col-md-11 content_left">
                                        <div class="form-group">
                                            <div class="col-md-12">
                                                <input type="text" placeholder="Mã hàng" class="form-control input-md" ng-model="mahang">
                                            </div>                                           
                                        </div>
                                        <!-- Text input-->
                                        <div class="form-group">
                                            <div class="col-md-12">
                                                <input type="text" placeholder="Tên hàng" class="form-control input-md" ng-model="tenhang">
                                            </div>

                                        </div>
                                        <!-- Text input-->
                                        <div class="form-group">
                                            <div class="col-md-12">
                                                <select ng-model="manhomhang" class="form-control input-md">
                                                    <option ng-selected="selectedValue == abc.MA_NHOM_HANG_CHI_TIET" ng-repeat="abc in danhsachnhomhang" value="{{abc.MA_NHOM_HANG_CHI_TIET}}">{{abc.CHUNG_LOAI_HANG}}</option>
                                                </select>
                                            </div>
                                        </div>
                                        <!-- Text input-->
                                        <div class="form-group">
                                            <div class="col-md-12">
                                                <input type="text" placeholder="Khối lượng" class="form-control input-md" ng-model="khoiluong">
                                            </div>
                                        </div>
                                        <!-- Text input-->
                                        <div class="form-group">
                                            <div class="col-md-12">
                                                <input type="text" placeholder="Đơn vị tính" class="form-control input-md" ng-model="donvitinh">
                                            </div>
                                        </div>
                                        <!-- Text input-->
                                        <div class="form-group">
                                            <div class="col-md-12">
                                                <input type="text" placeholder="Bảo hành" class="form-control input-md" ng-model="baohanh">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="col-md-12">
                                                <input type="text" placeholder="Giá list" class="form-control input-md" ng-model="gialist">
                                            </div>
                                        </div>
                                        <!-- Text input-->
                                        <div class="form-group">
                                            <div class="col-md-12">
                                                <input type="text" placeholder="Xuất xứ" class="form-control input-md" ng-model="xuatxu">
                                            </div>
                                        </div>
                                        <!-- Text input-->
                                        <div class="form-group">
                                            <div class="col-md-12">
                                                <input type="text" placeholder="Tài khoản hạch toán kho" class="form-control input-md" ng-model="tkhachtoankho">
                                            </div>
                                        </div>
                                        <!-- Text input-->
                                        <div class="form-group">
                                            <div class="col-md-12">
                                                <input type="text" placeholder="Tài khoản doanh thu" class="form-control input-md" ng-model="tkdoanhthu">
                                            </div>
                                        </div>
                                        <!-- Text input-->
                                        <div class="form-group">
                                            <div class="col-md-12">
                                                <input type="text" placeholder="Tài khoản chi phí" class="form-control input-md" ng-model="tkchiphi">
                                            </div>
                                        </div>

                                        <div class="form-group">
                                            <div class="col-md-12">
                                                <textarea id="ghichu" name="ghichu" placeholder="Ghi chú" class="form-control input-md" ng-model="ghichu"></textarea>
                                            </div>
                                        </div>
                                        <div class="form-group" style="margin-top: 10%">
                                            <form action="/Inventory/HangHoa" ng-controller="imgCtrl" method="post">
                                                <div class="col-md-3">
                                                    @Html.AntiForgeryToken()
                                                    <input type='file' name="files" id="imgInp" style="width: 100%" />
                                                    <input type="submit" value="Upload Ảnh hàng hóa" style="margin-top: 30px;" />
                                                </div>
                                                <div class="col-md-9">
                                                    <img id="blah" src="#" height="250" width="250" />
                                                </div>
                                            </form>
                                        </div>
                                    </div>
                                    <div style="padding-top:8px;" class="col-md-1">
                                        <i style="font-size:24px;" class="fa fa-question-circle nohelp_left" aria-hidden="true" onclick="help_left()"></i>
                                        <i style="font-size:24px;display: none" class="fa fa-ban help_left" aria-hidden="true" onclick="nohelp_left()"></i>
                                    </div>
                                </div>

                                <div class="col-md-4 help_left">
                                    <div class="form-group">
                                        <ul class="callouts">
                                            <li style="width:500px;height:50px;" class="callouts--left">Mã số hàng hóa là mã tiêu chuẩn thống nhất của mỗi hàng hóa trên toàn cầu </li>
                                        </ul>
                                    </div>
                                    <div class="form-group">
                                        <ul class="callouts">
                                            <li style="width:500px;height:50px;" callouts--left">Tên hàng hóa là tên tiêu chuẩn thống nhất của mỗi hàng hóa trên toàn cầu </li>
                                        </ul>
                                    </div>
                                    <div class="form-group">
                                        <ul class="callouts">
                                            <li style="width:500px;height:50px;" callouts--left">Mã nhóm hàng xác định hàng đó thuộc nhóm nào </li>
                                        </ul>
                                    </div>
                                    <div class="form-group">
                                        <ul class="callouts">
                                            <li style="width:500px;height:50px;" callouts--left">Khối lượng của hàng hóa đó </li>
                                        </ul>
                                    </div>
                                    <div class="form-group">
                                        <ul class="callouts">
                                            <li style="width:500px;height:50px;" callouts--left">Đơn vị mà người mua sẽ mua mặt hàng đó theo </li>
                                        </ul>
                                    </div>
                                    <div class="form-group">
                                        <ul class="callouts">
                                            <li style="width:500px;height:50px;" callouts--left">Thời gian bảo hành của hàng hóa đó </li>
                                        </ul>
                                    </div>
                                    <div class="form-group">
                                        <ul class="callouts">
                                            <li style="width:500px;height:50px;" callouts--left">Giá bán của hàng hóa đó </li>
                                        </ul>
                                    </div>
                                    <div class="form-group">
                                        <ul class="callouts">
                                            <li style="width:500px;height:50px;" callouts--left">Xuất xứ của hàng hóa đó </li>
                                        </ul>
                                    </div>
                                    <div class="form-group">
                                        <ul class="callouts">
                                            <li style="width:500px;height:50px;" callouts--left">Tài khoản hạch toán kho của hàng hóa đó </li>
                                        </ul>
                                    </div>
                                    <div class="form-group">
                                        <ul class="callouts">
                                            <li style="width:500px;height:50px;" callouts--left">Tài khoản doanh thu của hàng hóa đó </li>
                                        </ul>
                                    </div>
                                    <div class="form-group">
                                        <ul class="callouts">
                                            <li style="width:500px;height:50px;" callouts--left">Tài khoản chi phí của hàng hóa đó </li>
                                        </ul>
                                    </div>
                                    <div class="form-group">
                                        <ul class="callouts">
                                            <li style="width:500px;height:90px;" callouts--left">Ghi chú cho hàng hóa đó </li>
                                        </ul>
                                    </div>
                                    <div class="form-group" style="margin-top :15%">
                                        <ul class="callouts">
                                            <li style="width:500px;height:90px;" callouts--left">Hình ảnh của hàng hóa đó </li>
                                        </ul>
                                    </div>
                                </div>
                                <div class="col-md-6 container_right">
                                    <div class="col-md-12 content_right">                                        
                                        <div class="form-group" >
                                            <label class="col-md-2 control-label ckeditor" for="name">Đóng gói: </label>
                                            <div class="col-md-10 ckeditor">
                                                <textarea id="donggoi" name="donggoi" placeholder="Packing" class="form-control input-md" ng-model="quycachdonggoi"></textarea>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-2 control-label ckeditor" for="name"> Thông số kỹ thuật: </label>
                                            <div class="col-md-10 ckeditor">
                                                <textarea id="thongso" name="thongso" placeholder="Specification" class="form-control input-md" ng-model="thongso"></textarea>
                                            </div>
                                        </div>
                                    </div>
                                </div>                                                                                                                            
                                <script>
                                    CKEDITOR.replace('donggoi', {
                                        height: 150
                                    });
                                    CKEDITOR.replace('thongso', {
                                        height: 150
                                    });

                                </script>

                            </fieldset>
                    </div>
                    <div class="modal-footer">
                        <div class="form-group">
                            <label class="col-md-4 control-label" for="anmelden"></label>
                            <div class="col-md-8 addnew">
                                <button id="addnew" name="addnew" class="btn addnewmodal" ng-click="add()" data-dismiss="modal">Add New</button>
                                <button id="exit" name="exit" class="btn addnewmodal" data-dismiss="modal">Exit</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!--end modal-->
        <!-- Modal Update -->
        <div class="modal fade" id="myEdit" role="dialog" tabindex="-1">
            <div class="modal-dialog modal-lg" style="width: 90%">
                <div class="modal-content" style="height: 100%">

                    <div class="modal-body">
                            <fieldset style="height: 100%">
                                <!-- Form Name -->
                                <legend>Update Product</legend>
                                <!-- Text input-->
                                <div class="col-md-6" container_left>
                                    <div class="col-md-11">
                                        <div class="form-group">
                                            <label class="col-md-3 control-label" for="name">Mã hàng: </label>
                                            <div class="col-md-9">
                                                <input type="text" placeholder="Product Code" class="form-control input-md" ng-model="item.MA_HANG">
                                            </div>
                                        </div>
                                        <!-- Text input-->
                                        <div class="form-group">
                                            <label class="col-md-3 control-label" for="name">Tên hàng:</label>
                                            <div class="col-md-9">
                                                <input type="text" placeholder="Product Name" class="form-control input-md" ng-model="item.TEN_HANG">
                                            </div>
                                        </div>
                                        <!-- Text input-->
                                        <div class="form-group">
                                            <label class="col-md-3 control-label" for="name">Hãng: </label>
                                            <div class="col-md-9">
                                                <select ng-model="item.MA_NHOM_HANG" class="form-control input-md">
                                                    <option ng-repeat="abc in danhsachnhomhang" value="{{abc.MA_NHOM_HANG_CHI_TIET}}">{{abc.CHUNG_LOAI_HANG}}</option>
                                                </select>
                                            </div>
                                        </div>
                                        <!-- Text input-->
                                        <div class="form-group">
                                            <label class="col-md-3 control-label" for="name">Trọng lượng(g): </label>
                                            <div class="col-md-9">
                                                <input type="text" placeholder="Weight" class="form-control input-md" ng-model="item.KHOI_LUONG">
                                            </div>
                                        </div>
                                        <!-- Text input-->
                                        <div class="form-group">
                                            <label class="col-md-3 control-label" for="name">Đơn vị: </label>
                                            <div class="col-md-9">
                                                <input type="text" placeholder="Unit" class="form-control input-md" ng-model="item.DON_VI_TINH">
                                            </div>
                                        </div>
                                        <!-- Text input-->
                                        <div class="form-group">
                                            <label class="col-md-3 control-label" for="name">Bảo hành(tháng): </label>
                                            <div class="col-md-9">
                                                <input type="text" placeholder="Warranty" class="form-control input-md" ng-model="item.BAO_HANH">
                                            </div>
                                        </div>
                                        <!-- Text input-->
                                        <div class="form-group">
                                            <label class="col-md-3 control-label" for="name">Giá list: </label>
                                            <div class="col-md-9">
                                                <input type="text" placeholder="Giá list" class="form-control input-md" ng-model="item.GIA_LIST">
                                            </div>
                                        </div>
                                        <!-- Text input-->
                                        <div class="form-group">
                                            <label class="col-md-3 control-label" for="name">Xuất xứ: </label>
                                            <div class="col-md-9">
                                                <input type="text" placeholder="Native Place" class="form-control input-md" ng-model="item.XUAT_XU">
                                            </div>
                                        </div>
                                        <!-- Text input-->
                                        <div class="form-group">
                                            <label class="col-md-3 control-label no-pad-top" for="name">Tài khoản kho </label>
                                            <div class="col-md-9">
                                                <input type="text" placeholder="Inventory Account" class="form-control input-md" ng-model="item.TK_HACH_TOAN_KHO">
                                            </div>
                                        </div>
                                        <!-- Text input-->
                                        <div class="form-group">
                                            <label class="col-md-3 control-label" for="name">Tài khoản doanh thu: </label>
                                            <div class="col-md-9">
                                                <input type="text" placeholder="Sale Account" class="form-control input-md" ng-model="item.TK_DOANH_THU">
                                            </div>
                                        </div>
                                        <!-- Text input-->
                                        <div class="form-group">
                                            <label class="col-md-3 control-label" for="name">Tài khoản chi phí </label>
                                            <div class="col-md-9">
                                                <input type="text" placeholder="COGS Account" class="form-control input-md" ng-model="item.TK_CHI_PHI">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-3 control-label" for="name">Ghi chú: </label>
                                            <div class="col-md-9">
                                                <textarea id="ghichu" name="ghichu" placeholder="Note" class="form-control input-md" ng-model="item.GHI_CHU"></textarea>
                                            </div>
                                        </div>
                                        <div class="form-group" style="margin-top: 10%">
                                            <form action="/Inventory/HangHoa" ng-controller="imgCtrl" method="post">
                                                <div class="col-md-3">
                                                    @Html.AntiForgeryToken()
                                                    <input type='file' name="files" id="imgEdit" style="width: 100%" />
                                                    <input type="submit" value="Upload File/Image" style="margin-top: 30px;" />
                                                </div>
                                                <div class="col-md-9" style="box-shadow: 3px 3px 3px #ddd; width: auto" >
                                                    <img id="edit_img" src="#" height= 250 width="300" />
                                                </div>
                                            </form>
                                        </div>
                                    </div>
                                    <div style="padding-top:8px;" class="col-md-1">
                                        <i style="font-size:24px;" class="fa fa-question-circle nohelp_left" aria-hidden="true" onclick="help_left()"></i>
                                        <i style="font-size:24px;" class="fa fa-ban help_left" aria-hidden="true" onclick="nohelp_left()"></i>
                                    </div>
                                </div>
                                <div class="col-md-4 help_left">
                                    <div class="col-md-4 help_left">
                                        <div class="form-group">
                                            <ul class="callouts">
                                                <li style="width:500px;height:50px;" class="callouts--left">Mã số hàng hóa là mã tiêu chuẩn thống nhất của mỗi hàng hóa trên toàn cầu </li>
                                            </ul>
                                        </div>
                                        <div class="form-group">
                                            <ul class="callouts">
                                                <li style="width:500px;height:50px;" callouts--left">Tên hàng hóa là tên tiêu chuẩn thống nhất của mỗi hàng hóa trên toàn cầu </li>
                                            </ul>
                                        </div>
                                        <div class="form-group">
                                            <ul class="callouts">
                                                <li style="width:500px;height:50px;" callouts--left">Mã nhóm hàng xác định hàng đó thuộc nhóm nào </li>
                                            </ul>
                                        </div>
                                        <div class="form-group">
                                            <ul class="callouts">
                                                <li style="width:500px;height:50px;" callouts--left">Khối lượng của hàng hóa đó </li>
                                            </ul>
                                        </div>
                                        <div class="form-group">
                                            <ul class="callouts">
                                                <li style="width:500px;height:50px;" callouts--left">Đơn vị mà người mua sẽ mua mặt hàng đó theo </li>
                                            </ul>
                                        </div>
                                        <div class="form-group">
                                            <ul class="callouts">
                                                <li style="width:500px;height:50px;" callouts--left">Thời gian bảo hành của hàng hóa đó </li>
                                            </ul>
                                        </div>
                                        <div class="form-group">
                                            <ul class="callouts">
                                                <li style="width:500px;height:50px;" callouts--left">Giá bán của hàng hóa đó </li>
                                            </ul>
                                        </div>
                                        <div class="form-group">
                                            <ul class="callouts">
                                                <li style="width:500px;height:50px;" callouts--left">Xuất xứ của hàng hóa đó </li>
                                            </ul>
                                        </div>
                                        <div class="form-group">
                                            <ul class="callouts">
                                                <li style="width:500px;height:50px;" callouts--left">Tài khoản hạch toán kho của hàng hóa đó </li>
                                            </ul>
                                        </div>
                                        <div class="form-group">
                                            <ul class="callouts">
                                                <li style="width:500px;height:50px;" callouts--left">Tài khoản doanh thu của hàng hóa đó </li>
                                            </ul>
                                        </div>
                                        <div class="form-group">
                                            <ul class="callouts">
                                                <li style="width:500px;height:50px;" callouts--left">Tài khoản chi phí của hàng hóa đó </li>
                                            </ul>
                                        </div>
                                        <div class="form-group">
                                            <ul class="callouts">
                                                <li style="width:500px;height:90px;" callouts--left">Ghi chú cho hàng hóa đó </li>
                                            </ul>
                                        </div>
                                        <div class="form-group" style="margin-top :15%">
                                            <ul class="callouts">
                                                <li style="width:500px;height:90px;" callouts--left">Hình ảnh của hàng hóa đó </li>
                                            </ul>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 container_right">
                                    <div class="form-group">
                                        <label class="col-md-2 control-label ckeditor" for="name">Đóng gói: </label>
                                        <div class="col-md-10 ckeditor">
                                            <textarea id="editdonggoi" name="editdonggoi" placeholder="Đóng gói" class="form-control input-md" ng-model="item.QUY_CACH_DONG_GOI"></textarea>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-md-2 control-label ckeditor" for="name">Thông số: </label>
                                        <div class="col-md-10 ckeditor">
                                            <textarea id="editthongso" name="editthongso" placeholder="Details" class="form-control input-md" ng-model="item.THONG_SO_KY_THUAT"></textarea>
                                        </div>
                                    </div>
                                </div>
                                
                                <script>
                                    CKEDITOR.replace('editdonggoi', {
                                        height: 150
                                    });
                                    CKEDITOR.replace('editthongso', {
                                        height: 150
                                    });
                                </script>
                            </fieldset>
                    </div>
                    <div class="modal-footer">
                        <div class="form-group">
                            <label class="col-md-4 control-label" for="anmelden"></label>
                            <div class="col-md-2 edit">
                                <button class="btn" ng-click="save(item.MA_HANG)" data-dismiss="modal">Save</button>
                                <button class="btn" data-dismiss="modal">Exit</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <br />
        <!--end modal-->
        <!-- Modal delete -->
        <div class="modal fade" id="delete_item" role="dialog" tabindex="-1">
            <div class="modal-dialog modal-sm">
                <div class="modal-content">

                    <div class="modal-body">
                        <form class="form-horizontal" name="myForm" novalidate>
                            <fieldset>
                                <!-- Form Name -->
                                <legend>Delete Product</legend>
                                <center>Are you sure you want to delete <b>{{item.TEN_HANG}}</b></center>
                                <p></p>

                                <div class="form-group">
                                    <label class="col-md-4 control-label" for="delete"></label>
                                    <div class="col-md-8">
                                        <button id="delete" name="delete" class="btn" ng-click="delete(item.MA_HANG)" data-dismiss="modal">Delete</button>
                                        <button id="exit" name="exit" class="btn" data-dismiss="modal">Exit</button>
                                    </div>
                                </div>
                            </fieldset>
                        </form>
                    </div>
                </div>
            </div>
        </div>
        <br />
        <!--end modal-->
        <!--cHI TIẾT HÀNG HÓA-->
        <div class="modal fade" id="chitiethanghoa" role="dialog" tabindex="-1">
            <div class="modal-dialog modal-lg" style="width:80%;">
                <div class="modal-content" style="height: 600px;">

                    <div class="modal-body">
                        <div class="col-md-3">
                            <img ng-src="~/Content/Images/HangHoa/{{item.HINH_ANH}}" style="width: 90%;height: 250px;" />
                        </div>
                        <div class="col-md-9">
                            <div style="margin-bottom: 5%;" class="col-md-12">
                                <h1>{{item.TEN_HANG | uppercase}}</h1>
                                <div class="col-md-3 detail">Mã hàng: {{item.MA_HANG}}</div>
                                <div class="col-md-2 detail">Hãng: {{item.MA_NHOM_HANG}}</div>
                                <div class="col-md-2 detail">Xuất xứ: {{item.XUAT_XU}}</div>
                            </div>
                            <div>
                                <ul class="nav nav-tabs">
